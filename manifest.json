{
  "manifest_version": 3,
  "name": "Quick Password Generator",
  "description": "Generate strong random passwords right from your browser toolbar",
  "version": "1.0",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon48.png"
  },
  "permissions": [
    "clipboardWrite"
  ]
}